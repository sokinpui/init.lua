{
  "c": {
    "prefix": "c",
    "description": "",
    "body": [
      "```${1:C}",
      "$2",
      "```",
      "Output:",
      "```",
      "",
      "```"
    ]
  },
  "cb": {
    "prefix": "cb",
    "description": "",
    "body": [
      "```$2",
      "$1",
      "```",
      ""
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "nl": {
    "prefix": "nl",
    "description": "",
    "body": "<br>"
  },
  "com": {
    "prefix": "com",
    "description": "",
    "body": [
      "<!--",
      "${1:$TM_SELECTED_TEXT}",
      "-->"
    ]
  },
  "to": {
    "prefix": "to",
    "description": "",
    "body": [
      "<!--",
      "TODO: $0",
      "-->"
    ]
  },
  "li": {
    "prefix": "li",
    "description": "",
    "body": "[${1:$TM_SELECTED_TEXT}]($2)"
  },
  "img": {
    "prefix": "img",
    "description": "",
    "body": "![$1]($2)$0"
  },
  "gif": {
    "prefix": "gif",
    "description": "",
    "body": "![./img$1](img$1)"
  },
  "d": {
    "prefix": "d",
    "description": "",
    "body": "~~${1:$TM_SELECTED_TEXT}~~"
  },
  "_": {
    "prefix": "_",
    "description": "",
    "body": "_${1:$TM_SELECTED_TEXT}_"
  },
  "mail": {
    "prefix": "mail",
    "description": "",
    "body": [
      "Dear $1,",
      "",
      "$2",
      "",
      "Best Regards,",
      "So Kin Pui",
      "$0"
    ]
  },
  "dm": {
    "prefix": "dm",
    "description": "",
    "body": [
      "\\$\\$",
      "$1",
      "\\$\\$",
      "$0"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "x": {
    "prefix": "x",
    "description": "",
    "body": "\\times "
  },
  "/": {
    "prefix": "/",
    "description": "",
    "body": "\\frac{$1\\}{$2\\} $0"
  },
  "ll": {
    "prefix": "ll",
    "description": "",
    "body": "[[$0]]"
  },
  "col": {
    "prefix": "col",
    "description": "",
    "body": [
      "{{< details \"$1\" >\\}\\}",
      "$2",
      "{{< /details >\\}\\}"
    ]
  },
  "ref": {
    "prefix": "ref",
    "description": "",
    "body": "[$1]({{< ref \"/post/$2\" >\\}\\})"
  },
  "cen": {
    "prefix": "cen",
    "description": "",
    "body": [
      "{{< center >\\}\\} ",
      "${1:$TM_SELECTED_TEXT}",
      "{{< /center >\\}\\}"
    ]
  }
}
